busybox clear

busybox echo "Cleaning Ram.."
busybox sleep 1
busybox sync
busybox echo "3" >> /proc/sys/vm/drop_caches
busybox sleep 3
busybox echo "1" >> /proc/sys/vm/drop_caches


busybox echo "RAM cleaned"

exit
