		busybox echo "Disabling build.prop tweak..."
		busybox sleep 1
		busybox sed -i '/#Lock_default_launcher/d' $bpt
		busybox sed -i '/ro.HOME_APP_ADJ/d' $bpt
		busybox echo "Done!"
		busybox echo ""
		busybox echo "Please reboot your device!!!"
		busybox sleep 3
		sh system/bin/DBBuildProp
	else
		busybox echo "Enabling build.prop tweak..."
		busybox sleep 1
		 busybox sed -i '/#Lock_default_launcher/d' $bpt
		busybox sed -i '/ro.HOME_APP_ADJ/d' $bpt
		busybox sleep 1
		busybox echo "#Lock_default_launcher
ro.HOME_APP_ADJ=1
" >> $bpt
		busybox sed -i '/^$/d' $bpt
		busybox echo "Done!"
		busybox echo ""
		busybox echo "Please reboot your device!!!"
