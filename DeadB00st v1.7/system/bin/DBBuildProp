     busybox sed -i '/ro.media.dec.aud.wma.enabled/d' $bpt
		busybox sed -i '/ro.media.dec.vid.wmv.enabled/d' $bpt
		busybox sed -i '/ro.media.cam.preview.fps/d' $bpt
